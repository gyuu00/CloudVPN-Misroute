{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowVPCEOnlyAccessToS3",
      "Effect": "Allow",
      "Action": "s3:GetObject",
      "Resource": "arn:aws:s3:::cg-secret-flag/*",
      "Condition": {
        "StringEquals": {
          "aws:sourceVpce": "${vpce_id}"
        }
      }
    }
  ]
}
